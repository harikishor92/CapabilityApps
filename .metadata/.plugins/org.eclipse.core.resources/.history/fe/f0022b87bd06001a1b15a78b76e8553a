package com.capability.qa.Pages;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

import com.capability.qa.Base.BaseTest;

public class WelcomePage extends BaseTest{
	
	@FindBy(xpath = "//a[@class='nav-link login-btn']")
	private WebElement loginbtn;
	
	@FindBy(xpath = "//a[@class='navbar-brand']")
	private WebElement logotext;
	
	@FindBy(xpath = "//*[@class='svg-inline--fa fa-cart-plus fa-w-18 fa-2x']")
	private WebElement domaintab;
	
	@FindBy(xpath = "//*[@class='svg-inline--fa fa-outdent fa-w-14 fa-2x']")
	private WebElement technologytab;
	
	@FindBy(xpath = "//*[@class='svg-inline--fa fa-users fa-w-20 fa-2x']")
	private WebElement processtab;
	
	
}
